


<!doctype html>

<html lang="es">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
          integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">

    <title>Organicamente - Login</title>

</head>

<body>

<%- include('../compartido/estilos-generales/estilos-generales-component') %>
<%- include('../compartido/menu/menu-component', {usuarioId: id, esClienteC: esCliente, esProductorP:esProductor, esAdministradorA:esAdministrador}); -%>


<a href="/usuario-rol/vista/crear">
    <button>
        CREAR
    </button>
</a>

<table>
    <tr>
        <th>Usuario</th>
        <th>Rol</th>
        <!--   ESTO ES PARA UNA VISTA DE ADMINISTRADOR YA QUE SOLO INGRESA LOS ROLES -->
    </tr>

    <% arregloUsuariosRoles.forEach((objeto) => { %>
        <tr>
            <td><%= objeto.usuario.username %></td>
            <td><%= objeto.rol.nombre %></td>
            <td>
                <button>Ver</button>
                <f<%- include('../compartido/menu/menu-component') %>orm action="/usuario-rol/eliminarDesdeVista/<%= objeto.usuarioRolId %>" method="post">
                    <button>Eliminar</button>
                </form>
            </td>
        </tr>
    <% }) %>

</table>




<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
